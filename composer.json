{
    "name": "flammel/fission",
    "description": "Neos plugin to replace Fusion with Twig.",
    "type": "neos-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.3.0",
        "neos/neos": "^4.3|^5.0",
        "neos/neos-ui": "^4.0|^5.0",
        "twig/twig": "^3.0",
        "flammel/zweig": "@dev"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.19",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Flammel\\Fission\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flammel\\Fission\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Flammel.Fission"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:flammel/zweig.git"
        }
    ]
}
